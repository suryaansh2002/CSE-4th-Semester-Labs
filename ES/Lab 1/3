	AREA RESET, DATA, READONLY
	EXPORT __Vectors
__Vectors
		DCD 0X10001000
		DCD Reset_Handler
		ALIGN
		AREA mycode, CODE, READONLY
		ENTRY
		EXPORT Reset_Handler
Reset_Handler
	LDR R0,=SRC
	MOV R6,#10
	MOV R7,#1
PUT
	STR R7,[R0],#4
	ADD R7,#1
	SUBS R6,#1
	BNE PUT


	LDR R1,=SRC
	ADD R1,#36
	LDR R0,=SRC

	MOV R2,#5
LOOP
	LDR R3,[R0]
	LDR R4,[R1]
	STR R3 , [R1],#-4
	STR R4 , [R0],#4
	SUBS R2,#1
	BNE LOOP

STOP
	B STOP
	

	AREA mydata,DATA,READWRITE
SRC DCD 0
	END
